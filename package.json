{
  "name": "uniprint",
  "version": "1.0.0",
  "description": "Printserver for Unitrend",
  "main": "./src/index.ts",
  "type": "module",
  "scripts": {
    "start": "node src/index.ts",
    "watch": "node --watch src/index.ts",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "prepare": "husky",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tizzyapunkt/uniprint.git"
  },
  "author": "Tizian Adam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tizzyapunkt/uniprint/issues"
  },
  "homepage": "https://github.com/tizzyapunkt/uniprint#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.11.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  }
}
